{
  "name": "arboretum",
  "version": "0.0.0",
  "description": "General tree manipulator",
  "main": "index.js",
  "scripts": {
    "test": "mocha test -- --require babel-register --require --babel-polyfill",
    "build-prod": "NODE_ENV=production babel src --out-dir bin/prod",
    "build-dev": "NODE_ENV=development babel src --out-dir bin/dev",
    "build": "npm run build-prod && npm run build-dev",
    "preversion": "npm test",
    "postversion": "npm run build && npm publish && git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyeonupark/arboretum.git"
  },
  "keywords": [
    "tree",
    "ast",
    "node"
  ],
  "author": "HyeonuPark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyeonupark/arboretum/issues"
  },
  "homepage": "https://github.com/hyeonupark/arboretum#readme",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "iterable-queue": "^1.0.2",
    "iterator-util": "^1.1.0"
  }
